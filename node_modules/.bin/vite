#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules/vite/bin/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules/vite/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/node_modules:/Users/goroman/Work/Windsurf/node_modules:/Users/goroman/Work/node_modules:/Users/goroman/node_modules:/Users/node_modules:/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules/vite/bin/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules/vite/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/node_modules:/Users/goroman/Work/Windsurf/node_modules:/Users/goroman/Work/node_modules:/Users/goroman/node_modules:/Users/node_modules:/node_modules:/Users/goroman/Work/Windsurf/GPT4.1/Quiz2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
